{
  "name": "redux-form-async-validator",
  "version": "1.0.1",
  "description": "Easy asynchronous validation of multiple redux-form fields",
  "main": "dist/index.js",
  "author": "Sajad Torkamani <sajadtorkamani1@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "react-redux",
    "redux-form",
    "form",
    "form-validation",
    "validation",
    "asynchronous-validation"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.4.0",
    "jest": "^23.5.0",
    "jest-extended": "^0.8.1"
  },
  "scripts": {
    "test": "jest --notify --detectOpenHandles",
    "watch": "babel src -d dist --watch",
    "build": "babel src -d dist",
    "npm:pack-remove": "rm -rf redux-form-*.tgz package/",
    "npm:pack-generate": "npm pack && tar -xvzf redux-form-*.tgz",
    "npm:pack": "npm run npm:pack-remove && npm run npm:pack-generate"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/sajadtorkamani/redux-form-async-validator.git"
  }
}
